<Window x:Class="AIPO74_HFT_2021221.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIPO74_HFT_2021221.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Content="Crud" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Center" Height="40" Width="80" Click="JumpToCrudButton" ></Button>
        <Button Content="NON-Crudes" HorizontalAlignment="Center" Grid.Row="2" VerticalAlignment="Center" Height="40" Width="80" Click="ToNoncrudForm" ></Button>
        <Button Content="Exit" HorizontalAlignment="Center" Grid.Row="3" VerticalAlignment="Center" Height="40" Width="80" Click="ExitMainButton" ></Button>

    </Grid>
</Window>
